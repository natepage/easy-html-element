{
    "name": "natepage/easy-html-element",
    "type": "library",
    "description": "An easy way to create simple or complex html elements in PHP.",
    "keywords": [],
    "license": "MIT",
    "homepage": "https://github.com/natepage/easy-html-element",
    "authors": [
        {
            "name": "Nathan Page",
            "email": "nathan.page86@gmail.com",
            "homepage": "https://github.com/natepage",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "airmanbzh/php-html-generator": "dev-master",
        "zendframework/zend-escaper": "^2.5"
    },
    "autoload": {
        "psr-4": { "NatePage\\EasyHtmlElement\\": "src/NatePage/EasyHtmlElement/" }
    },
    "autoload-dev": {
        "psr-4": { "NatePage\\EasyHtmlElement\\Tests\\": "tests/NatePage/EasyHtmlElement/Tests/" }
    },
    "suggest": {
        "symfony/yaml": "Allows you to define the elements map in YAML to make it easier !"
    },
    "support": {
        "issues": "https://github.com/natepage/easy-html-element/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
